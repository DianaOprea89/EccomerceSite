{"ast":null,"code":"import api from \"@/api/api\";\nimport ProductsList from \"@/components/ProductsList\";\nexport default {\n  name: \"CartPage\",\n  components: {\n    ProductsList\n  },\n  data() {\n    return {\n      cartItems: [] // Initialize with an empty array\n    };\n  },\n\n  computed: {\n    totalPrice() {\n      if (!Array.isArray(this.cartItems)) {\n        return 0; // Return 0 or any default value when cartItems is not an array\n      }\n\n      return this.cartItems.reduce((sum, item) => sum + Number(item.price), 0);\n    },\n    computedCartItems() {\n      return this.$store.getters.cartItems;\n    }\n  },\n  created() {\n    this.fetchData();\n  },\n  methods: {\n    async removeFromCart(productId) {\n      const userId = this.$store.getters.loggedInUser.id;\n      try {\n        const response = await api.delete(`/api/users/${userId}/cart/${productId}`);\n        this.cartItems = response.data;\n      } catch (error) {\n        console.error('Error removing item from cart:', error);\n      }\n    },\n    async fetchData() {\n      if (this.$store.getters.isAuthenticated) {\n        const userId = this.$store.getters.loggedInUser.id;\n        console.log('Fetching cart data for userId:', userId);\n        try {\n          const response = await api.get(`/api/users/${userId}/cart`);\n          console.log('Response data:', response.data);\n          this.cartItems = response.data; // Update cartItems with the fetched data\n        } catch (error) {\n          console.error('Error fetching cart items:', error);\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["api","ProductsList","name","components","data","cartItems","computed","totalPrice","Array","isArray","reduce","sum","item","Number","price","computedCartItems","$store","getters","created","fetchData","methods","removeFromCart","productId","userId","loggedInUser","id","response","delete","error","console","isAuthenticated","log","get"],"sources":["src/views/CartPage.vue"],"sourcesContent":["<template>\r\n  <div id=\"page-wrap\">\r\n    <h1>Shopping Cart</h1>\r\n    <ProductsList\r\n        :products=\"cartItems\"\r\n        v-on:remove-from-cart=\"removeFromCart($event)\"\r\n    ></ProductsList>\r\n    <div>\r\n      <h3 id=\"total-price\">Total: ${{ totalPrice }}</h3>\r\n      <button id=\"checkout-button\">Proceed to Checkout</button>\r\n    </div>\r\n  </div>\r\n\r\n</template>\r\n<script>\r\nimport api from \"@/api/api\";\r\nimport ProductsList from \"@/components/ProductsList\";\r\n\r\nexport default {\r\n  name: \"CartPage\",\r\n  components: { ProductsList },\r\n  data() {\r\n    return {\r\n      cartItems: [], // Initialize with an empty array\r\n    };\r\n  },\r\n  computed: {\r\n    totalPrice() {\r\n      if (!Array.isArray(this.cartItems)) {\r\n        return 0; // Return 0 or any default value when cartItems is not an array\r\n      }\r\n      return this.cartItems.reduce(\r\n          (sum, item) => sum + Number(item.price),\r\n          0\r\n      );\r\n    },\r\n    computedCartItems() {\r\n      return this.$store.getters.cartItems;\r\n    },\r\n  },\r\n  created() {\r\n    this.fetchData();\r\n  },\r\n  methods: {\r\n    async removeFromCart(productId) {\r\n      const userId = this.$store.getters.loggedInUser.id;\r\n      try {\r\n        const response = await api.delete(\r\n            `/api/users/${userId}/cart/${productId}`\r\n        );\r\n        this.cartItems = response.data;\r\n      } catch (error) {\r\n        console.error('Error removing item from cart:', error);\r\n      }\r\n    },\r\n    async fetchData() {\r\n      if (this.$store.getters.isAuthenticated) {\r\n        const userId = this.$store.getters.loggedInUser.id;\r\n        console.log('Fetching cart data for userId:', userId);\r\n        try {\r\n          const response = await api.get(`/api/users/${userId}/cart`);\r\n          console.log('Response data:', response.data);\r\n          this.cartItems = response.data; // Update cartItems with the fetched data\r\n        } catch (error) {\r\n          console.error('Error fetching cart items:', error);\r\n        }\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nh1 {\r\n  border-bottom: 1px solid black;\r\n  margin: 0;\r\n  margin-top: 16px;\r\n  padding: 16px;\r\n}\r\n\r\n#total-price {\r\n  padding: 16px;\r\n  text-align: right;\r\n}\r\n\r\n#checkout-button {\r\n  width: 100%;\r\n}\r\n\r\n.product-container {\r\n  align-content: center;\r\n  border-bottom: 1px solid #ddd;\r\n  display: flex;\r\n  padding: 16px;\r\n  width: 100%;\r\n}\r\n\r\n.product-image {\r\n  flex: 1;\r\n  height: 100px;\r\n  max-width: 100px;\r\n}\r\n\r\n.details-wrap {\r\n  padding: 0 16px;\r\n  flex: 3;\r\n}\r\n\r\n.remove-button {\r\n  flex: 1;\r\n  margin: auto;\r\n}\r\n</style>\r\n"],"mappings":"AAeA,OAAAA,GAAA;AACA,OAAAC,YAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IAAAF;EAAA;EACAG,KAAA;IACA;MACAC,SAAA;IACA;EACA;;EACAC,QAAA;IACAC,WAAA;MACA,KAAAC,KAAA,CAAAC,OAAA,MAAAJ,SAAA;QACA;MACA;;MACA,YAAAA,SAAA,CAAAK,MAAA,CACA,CAAAC,GAAA,EAAAC,IAAA,KAAAD,GAAA,GAAAE,MAAA,CAAAD,IAAA,CAAAE,KAAA,GACA,CACA;IACA;IACAC,kBAAA;MACA,YAAAC,MAAA,CAAAC,OAAA,CAAAZ,SAAA;IACA;EACA;EACAa,QAAA;IACA,KAAAC,SAAA;EACA;EACAC,OAAA;IACA,MAAAC,eAAAC,SAAA;MACA,MAAAC,MAAA,QAAAP,MAAA,CAAAC,OAAA,CAAAO,YAAA,CAAAC,EAAA;MACA;QACA,MAAAC,QAAA,SAAA1B,GAAA,CAAA2B,MAAA,CACA,cAAAJ,MAAA,SAAAD,SAAA,EACA;QACA,KAAAjB,SAAA,GAAAqB,QAAA,CAAAtB,IAAA;MACA,SAAAwB,KAAA;QACAC,OAAA,CAAAD,KAAA,mCAAAA,KAAA;MACA;IACA;IACA,MAAAT,UAAA;MACA,SAAAH,MAAA,CAAAC,OAAA,CAAAa,eAAA;QACA,MAAAP,MAAA,QAAAP,MAAA,CAAAC,OAAA,CAAAO,YAAA,CAAAC,EAAA;QACAI,OAAA,CAAAE,GAAA,mCAAAR,MAAA;QACA;UACA,MAAAG,QAAA,SAAA1B,GAAA,CAAAgC,GAAA,eAAAT,MAAA;UACAM,OAAA,CAAAE,GAAA,mBAAAL,QAAA,CAAAtB,IAAA;UACA,KAAAC,SAAA,GAAAqB,QAAA,CAAAtB,IAAA;QACA,SAAAwB,KAAA;UACAC,OAAA,CAAAD,KAAA,+BAAAA,KAAA;QACA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}